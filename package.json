{
	"name": "angular-cli-lib-example",
	"version": "0.0.0",
	"license": "MIT",
	"author": "Koen Zigterman <koen@zigterman.com> (http://zigterman.com)",
	"description": "An Animating Datepicker for Angular 2+, for some smooth date picking :).",
	"keywords": [
		"angular",
		"datepicker",
		"animating",
		"ngx",
		"ng2",
		"angular 2"
	],
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"build:lib": "ng-packagr -p libs/ngx-tabs-lib/package.json",
		"test": "ng test",
		"lint:fix": "ng lint --fix",
		"e2e": "ng e2e",
		"prettier": "prettier --single-quote --write --print-width=120 --tab-width 4 'src/app/**/*.ts'",
		"tslint": "tslint --format stylish 'src/app/**/*.ts'",
		"csslint": "stylelint \"**/*.scss\"",
		"cssfix": "stylelint \"**/*.scss\" --fix",
		"apps:affected": "./node_modules/.bin/nx affected apps",
		"build:affected": "./node_modules/.bin/nx affected build",
		"e2e:affected": "./node_modules/.bin/nx affected e2e",
		"affected:apps": "./node_modules/.bin/nx affected apps",
		"affected:build": "./node_modules/.bin/nx affected build",
		"affected:e2e": "./node_modules/.bin/nx affected e2e",
		"format": "./node_modules/.bin/nx format write",
		"format:write": "./node_modules/.bin/nx format write",
		"format:check": "./node_modules/.bin/nx format check",
		"nx-migrate": "./node_modules/.bin/nx migrate"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "5.1.0",
		"@angular/common": "5.1.0",
		"@angular/compiler": "5.1.0",
		"@angular/core": "5.1.0",
		"@angular/forms": "5.1.0",
		"@angular/platform-browser": "5.1.0",
		"@angular/platform-browser-dynamic": "5.1.0",
		"@angular/router": "5.1.0",
		"@nrwl/nx": "0.6.19",
		"bootstrap": "^4.0.0",
		"core-js": "^2.4.1",
		"prismjs": "^1.11.0",
		"rxjs": "^5.5.2",
		"zone.js": "^0.8.14"
	},
	"devDependencies": {
		"@angular-devkit/core": "^0.0.28",
		"@angular/cli": "^1.7.3",
		"@angular/compiler-cli": "5.1.0",
		"@angular/language-service": "5.1.0",
		"@nrwl/schematics": "0.6.19",
		"@types/jasmine": "~2.5.53",
		"@types/jasminewd2": "~2.0.2",
		"@types/node": "~6.0.60",
		"codelyzer": "^4.0.1",
		"csslint": "^1.0.5",
		"jasmine-core": "~2.6.2",
		"jasmine-spec-reporter": "~4.1.0",
		"karma": "~1.7.0",
		"karma-chrome-launcher": "~2.1.1",
		"karma-cli": "~1.0.1",
		"karma-coverage-istanbul-reporter": "^1.2.1",
		"karma-jasmine": "~1.1.0",
		"karma-jasmine-html-reporter": "^0.2.2",
		"ng-packagr": "^2.0.0-rc.11",
		"prettier": "^1.11.1",
		"protractor": "~5.1.2",
		"stylelint": "^9.1.3",
		"stylelint-config-recommended": "^2.1.0",
		"stylelint-webpack-plugin": "^0.10.3",
		"ts-node": "~3.2.0",
		"tslint": "~5.7.0",
		"typescript": "2.5.3"
	}
}
